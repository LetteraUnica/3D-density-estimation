    "def get_cell(x, N):\n",
    "    return (int)(N*x)\n",
    "\n",
    "def get_point(Nx, N):\n",
    "    return (Nx + 0.5) / N\n",
    "\n",
    "\n",
    "def low_bound(x, N):\n",
    "    Nx = get_cell(x, N)\n",
    "    point = get_point(Nx, N)\n",
    "\n",
    "    if point > x:\n",
    "        res = Nx\n",
    "    else:\n",
    "        res = Nx + 1\n",
    "\n",
    "    return res\n",
    "\n",
    "N = 5\n",
    "\n",
    "low_bound(0.31, N)"
   ]
  }
